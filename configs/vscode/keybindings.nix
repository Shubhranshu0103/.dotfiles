[
  {
    key = "ctrl+shift+/";
    command = "workbench.action.terminal.newWithCwd";
    args = {
      cwd = "\${fileDirname}";
    };
  }

  {
    key = "ctrl+space w";
    command = "workbench.action.switchWindow";
  }

  {
    key = "ctrl+space q w";
    command = "workbench.action.quickSwitchWindow";
  }
]